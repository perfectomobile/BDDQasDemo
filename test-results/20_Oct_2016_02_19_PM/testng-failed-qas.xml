<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="100" name="QAF-Gherkin-Demo" parallel="tests">
  <test name="GherkinTest">
    <method-selectors>
      <method-selector>
        <script language="beanshell">
          <![CDATA[testngMethod.getMethodName().equalsIgnoreCase("Search QMetry")]]>
        </script>
      </method-selector>
    </method-selectors>
    <classes>
      <class name="com.qmetry.qaf.automation.step.client.text.BDDTestFactory"/>
    </classes>
  </test> <!-- GherkinTest -->
</suite> <!-- QAF-Gherkin-Demo -->
